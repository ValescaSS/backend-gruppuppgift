<body>

  <template id="loginFormTemplate">
    <div id="hideLoginForm">
      <h2>Logga in användare</h2>
      <form id="loginForm">
        <div id="wrongPassAndUserErrorMsg" class="errMsg"></div>
        <div id="noRegisteredNameErrorMsg" class="errMsg"></div>
        <input type="text" name="username" id="usernameLogin" placeholder="Username">
        <div id="wrongPassErrorMsg" class="errMsg"></div>
        <input type="password" name="password" id="passwordLogin" placeholder="Password">
        <input type="submit" value="Logga in">
      </form>
    </div>
  </template>

  <template id="registerFormTemplate">
    <div id="hideRegisterForm">
      <h2>Registrera användare</h2>
      <form id="registerForm">
        <div id="nameAndPassErrorMsg" class="errMsg"></div>
        <div id="nameErrorMsg" class="errMsg"></div>
        <input type="text" name="username" id="usernameRegister" placeholder="Username">
        <div id="PassErrorMsg" class="errMsg"></div>
        <input type="password" name="password" id="passwordRegister" placeholder="Password">

        <input type="submit" value="Register">
      </form>
    </div>
  </template>

  <template id="createEntryFormTemplate">
    <div id="showEntriesForm" class="hidden">
      <form id="entriesForm">
        <input type="text" name="title" id="title">
        <textarea name="content" id="content" cols="30" rows="10"></textarea>
        <input type="submit" value="Add">
      </form>
    </div>
  </template>

  <template id="createEntrySuccessTemplate">

  </template>

  <template id="editEntry">
    <form id="editEntryForm">
      <div>
        <label for="title">Title</label>
        <input type="text" name="title" id="edit-title" class="form-control" value="">

        <label for="content">Content</label>
        <textarea type="text" name="content" rows="10" id="edit-content"></textarea>

        <input type="submit" value="Edit" rolr="button" id="send-edit">
      </div>
    </form>
  </template>

  <template id="lastTwentyEntriesTemplate">
    <div id="senasteEntries" style="border:1px solid black !important;">
      <!-- <h2>Senaste 20 inläggen</h2> -->
      <!-- innehåll -->
    </div>
  </template>

  <template id="showCompleteEntryTemplate">
    <div id="completeEntry">
    </div>
  </template>

  <template id="entryCommentsTemplates">
    <div id="entryComments">
      <h2>Comments</h2>
      <!-- innehåll -->
    </div>
    <a href="" id="hemLink">Tillbaka</a>
  </template>

  <div id="user-div"></div>
  <template id="createCommentFormTemplate">

     <form id="commentForm">
     <br>
       <textarea name="content" id="content" cols="30" rows="10" placeholder="Comment...."></textarea>
       <input type="submit" value="Add comment">
       
     </form>

  </template>

  <template id="moreentryCommentsTemplates">
   <h2>Comments</h2>
   <div id="moreentryComments" >
     <!-- innehåll -->
   </div>
   
 </template>

 <template id="editCommentTemplate">
 <form id="editCommentForm">
       <textarea name="content" id="editCommentContent" cols="30" rows="10" value="" ></textarea>
       <input type="submit" value="Add comment">
     </form>     

 </template>

  

  <div class="container">
    <a href="" id="logout" class="hidden">Logga ut</a>
    <!-- <button id="logout" class="hidden">Logga ut</button> -->
    <header>
      <h1><?=$title?></h1>
    </header>

    <main>
      <!-- innehåll -->
